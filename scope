local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/polar"))()
local ok = library:CreateWindow("Doomware - Noscope Arcade")
ok:Section("Aimbot")
ok:Toggle("AIMBOT",function(a)
    getgenv().Settings = {
   Fov = 999999,
   Hitbox = "Head",
   FovCircle = true,
}
 
loadstring(game:HttpGet("https://raw.githubusercontent.com/WetCheezit/Releases/main/No-Scope-Arcade/Silent%20aim.lua"))()
end)

ok:Toggle("ESP",function(a)
loadstring(game:HttpGet("https://raw.githubusercontent.com/buqs9999/esp/main/esp"))()
end)
